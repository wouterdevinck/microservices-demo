{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "removeComments": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist"
  },
  "include": [
    "src/*.ts"
  ]
}